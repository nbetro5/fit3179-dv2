{
  "$schema": "https://vega.github.io/schema/vega-lite/v6.json",
  "title": {"text": "Australian Car Sales Over Time"},
  "width": 600,
  "height": 600,
  "data": {"url": "https://raw.githubusercontent.com/nbetro5/fit3179-dv2/main/testdatastate.csv", "format": {"type": "csv"}},
  "mark": {
    "type": "line",
    "point": {
      "filled": false,
      "fill": "white"
    }
  },
  "encoding": {
    "x": {"timeUnit": "year", "field": "Year", "title": "Year"},
    "y": {"field": "Sales", "type": "quantitative", "aggregate": "sum", "title": "Total Sales"},
    "color": {"field": "symbol", "type": "nominal"}
  }
}