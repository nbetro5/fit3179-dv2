{
  "$schema": "https://vega.github.io/schema/vega-lite/v6.json",
  "width": 600,
  "height": 600,
  "data": {"url": //data source goes here},
  "mark": "bar",
  "encoding": {
    "x": {
      "timeUnit": "year",
      "field": "Year",
      "type": "ordinal",
      "title": "Year"
    },
    "y": {
      "aggregate": "count",
      "type": "quantitative"
    },
    "color": {
      "field": "Powerplant",
      "type": "nominal",
      "scale": {
        "domain": ["EV", "Hybrid", "ICE"],
        "range": ["#a6cee3","#1f78b4","#b2df8a"]
      }
    }
  }
},
"title": "Vehicle Sales by Powerplant"
}